<% if logged_in? %>
  <li class="nav-item">
    <%= link_to t('logout.title').capitalize, '#', class: 'nav-link' %>
  </li>
<% else %>
  <li class="nav-item">
    <%= link_to t('signup').capitalize, signup_url, class: 'nav-link' %>
  </li>
  <li class="nav-item">
    <%= link_to t('log_in.title').capitalize, login_url, class: 'nav-link' %>
  </li>
<% end %>
